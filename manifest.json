{
  "name": "Delete MY Cookies",
  "version": "1",
  "manifest_version": 1,
  "description": "Delete MY specific cookies.",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
  "background": {
    "scripts": ["delete_cookie.js"]
  },
  "permissions": [
    "cookies",
    "*://*/*"
  ]
}
